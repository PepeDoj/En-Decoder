<Page x:Class="En_Decoder.SignInPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:En_Decoder"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="SignInPage">

    <Grid>
        <TextBox x:Name="LoginSignIn" HorizontalAlignment="Left" Margin="300,70,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="NicknameSignIn" HorizontalAlignment="Left" Margin="300,100,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <PasswordBox x:Name="PasswordSignIn" HorizontalAlignment="Left" Margin="300,130,0,0" VerticalAlignment="Top" Width="120"/>
        <PasswordBox x:Name="PasswordProofSignIn" HorizontalAlignment="Left" Margin="300,160,0,0" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="SignIn" Content="Button" HorizontalAlignment="Left" Margin="300,190,0,0" VerticalAlignment="Top" Width="120" Click="SignIn_Click" />
    </Grid>
</Page>
