<Page x:Class="En_Decoder.SignInPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:En_Decoder"
      mc:Ignorable="d" 
      d:DesignHeight="534
      " d:DesignWidth="875"
      Title="SignInPage">

    <Grid>
        <TextBox x:Name="LoginSignUp" HorizontalAlignment="Center" Margin="0,0,0,25" TextWrapping="Wrap" VerticalAlignment="Center" Width="120" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
        <Button x:Name="SignUp" Content="Button" HorizontalAlignment="Center" Margin="0,225,0,100" VerticalAlignment="Center" Width="120" Click="SignUp_Click" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
        <PasswordBox x:Name="PasswordSignUp" HorizontalAlignment="Center" VerticalAlignment="Center" Width="120" Margin="0,50,0,0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
    </Grid>
</Page>
