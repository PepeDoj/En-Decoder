<Window x:Class="En_Decoder.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:En_Decoder"
        mc:Ignorable="d"
        
        Title="MainWindow" Height="550" Width="1000">
    <Grid>
        <Button x:Name="Enter" Content="Button" HorizontalAlignment="Left" Margin="774,449,0,0" VerticalAlignment="Top" Height="38" Width="50" RenderTransformOrigin="0.48,0.554" Click="Enter_Click"/>
        <TextBox x:Name="Chat" HorizontalAlignment="Left" Margin="28,28,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="741" Height="390" FontSize="20" AcceptsTab="True" AcceptsReturn="True"/>
        <TextBox x:Name="Message" HorizontalAlignment="Left" Margin="28,423,0,28" TextWrapping="Wrap" Width="741" FontSize="20" />
        <TextBox x:Name="NicknameSignIn" HorizontalAlignment="Left" Margin="799,28,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <PasswordBox x:Name="PasswordSignIn" HorizontalAlignment="Left" Margin="799,134,0,0" VerticalAlignment="Top" Width="120"/>
        <PasswordBox x:Name="PasswordProofSignIn" HorizontalAlignment="Left" Margin="799,173,0,0" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="SignIn" Content="Button" HorizontalAlignment="Left" Margin="799,223,0,0" VerticalAlignment="Top" Width="120" Click="SignIn_Click" />
        <TextBox x:Name="LoginSignIn" HorizontalAlignment="Left" Margin="799,85,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="LoginSignUp" HorizontalAlignment="Left" Margin="799,286,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="SignUp" Content="Button" HorizontalAlignment="Left" Margin="799,377,0,0" VerticalAlignment="Top" Width="120" Click="SignUp_Click"/>
        <PasswordBox x:Name="PasswordSignUp" HorizontalAlignment="Left" Margin="799,334,0,0" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
