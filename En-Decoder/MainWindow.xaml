<Window x:Class="En_Decoder.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:En_Decoder"
        mc:Ignorable="d"
        
        Title="MainWindow" Height="550" Width="1000">
    <Grid>
        <StackPanel Margin="0,0,875,0">
            <Button x:Name="Open" Content="Вход" Margin="0,58,0,15" Click="Open_Click"/>
            <Button x:Name="Chat" Content="Button" Margin="0,0,0,15" Click="Chat_Click"/>
            <Button Content="Button"/>
        </StackPanel>

        <Frame x:Name="MainFrame" Content="" Margin="125,0,0,0" NavigationUIVisibility="Hidden"/>
        <Label x:Name="UserLabel" Content="{Binding DataBanr.UserLog}" Margin="223,180,483,189"/>


    </Grid>
</Window>
